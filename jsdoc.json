{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    },
    "opts": {
        "private": false,
        "protected": false,
        "template": "templates/default",
        "encoding": "utf8",
        "recurse": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "./lib/xblocks-core/src"
        ],
        "includePattern": ".+\\.js(doc)?$"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
