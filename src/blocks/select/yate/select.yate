module "xb-select"
import "xblocks"


match / xb-select {
<xb-select>
    apply .attrs.* xb-core-attrs
    .content
</xb-select>
}


match / template {
<label>
    <xb-button>
        apply .attrs.button.attrs.* xb-core-attrs
    </xb-button>
    <div class="" style="position: absolute;background: white;min-width: 100px;border: 1px solid;">
        apply . xb-core-content
    </div>
</label>
}

