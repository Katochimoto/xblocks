module "xb-select"
import "xblocks"


match / xb-select {
<xb-select>
    apply .attrs.* xb-core-attrs
    .content
</xb-select>
}


match / template {
<label>
    <xb-button>
        apply .attrs.button.attrs.* xb-core-attrs
    </xb-button>

    <input type="text" tabindex="0" disabled="disabled" />


    apply . xb-core-content
</label>
}

