module "xb-select"
import "xblocks"

match .* block-class {

}


match / xb-select {
<xb-select>
    apply .attrs.* xb-core-attrs
    .content
</xb-select>
}


match / template {
<div class="xb-select">
    apply .attrs.* block-class
    <xb-button>
        apply .attrs.button.attrs.* xb-core-attrs
    </xb-button>
    apply . xb-core-content
</div>
}

